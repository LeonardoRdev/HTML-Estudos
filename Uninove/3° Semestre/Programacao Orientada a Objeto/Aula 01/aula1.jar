public class Main {
    public static void main(String[] args) {
        //**OPERADORES ARITMÉTICOS**
        //Adição (+), Subtração (-), Divisão(/), Multiplicação(*)
        //Módulo "REST" (%)
        int v1 = 10;
        int v2 = 3;
        int r1 = v1 + v2;
        int r2 = v1 - v2;
        int r3 = v1 * v2;
        int r4 = v1 / v2;
        int r5 = v1 % v2;
        int r6 = v1 + 5;
        System.out.println("Adição: " + r1);
        System.out.println("Subtração: " + r2);
        System.out.println("Multiplicação: " + r3);
        System.out.println("Divisão: " + r4);
        System.out.println("Módulo (RESTO): " + r5);
        System.out.println("Elevação Pélvica: " + v2 + " séries de " + r6 + " repetições | Com aula gratuita do professor Leo");
}}